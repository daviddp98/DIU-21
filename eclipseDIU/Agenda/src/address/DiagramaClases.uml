            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_mVoxMB31EeuDJ68x4ifRQQ" name="setPerson" specification="_mVm8AB31EeuDJ68x4ifRQQ">
              <language>JAVA</language>
              <body>&#xD;
    this.person = person;&#xD;
    firstNameField.setText(person.getFirstName());&#xD;
