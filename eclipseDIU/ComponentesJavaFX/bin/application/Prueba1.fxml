<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="250.0" prefWidth="213.0"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
	<children>
		<VBox prefHeight="310.0" prefWidth="288.0">
			<children>
				<Label alignment="CENTER" contentDisplay="CENTER"
					graphicTextGap="5.0" prefHeight="317.0" prefWidth="288.0"
					text="Ejemplo de aplicacion utilizando FXML" textAlignment="CENTER"
					textOverrun="CLIP" wrapText="true">
					<font>
						<Font name="Impact" size="30.0" />
					</font>
				</Label>
			</children>
		</VBox>
		<HBox fx:id="botonPrueba" layoutX="192.0" layoutY="354.0"
			prefHeight="56.0" prefWidth="96.0">
			<children>
				<Button id="botonCerrar" fx:id="botonCerrar"
					alignment="CENTER" mnemonicParsing="false" onAction="#cerrar"
					prefHeight="40.0" prefWidth="90.0"
					style="-fx-background-color: ff0000;" text="Cerrar"
					textAlignment="CENTER" textFill="WHITE">
					<font>
						<Font name="System Bold" size="18.0" />
					</font>
				</Button>
			</children>
		</HBox>
	</children>
</AnchorPane>